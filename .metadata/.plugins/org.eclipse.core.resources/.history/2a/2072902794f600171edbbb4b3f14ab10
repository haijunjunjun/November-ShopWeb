package com.november.common.token;import org.springframework.boot.context.properties.ConfigurationProperties;import org.springframework.boot.context.properties.EnableConfigurationProperties;import org.springframework.context.annotation.PropertySource;import org.springframework.stereotype.Component;import lombok.Data;import lombok.Getter;import lombok.Setter;@Data@Component@EnableConfigurationProperties@ConfigurationProperties(prefix = "november.token.jwt")@PropertySource(value = { "classpath:/security-jwt.properties" }, ignoreResourceNotFound = true)public class TokenConfig {		 @Getter @Setter private  String issuer;	 @Getter @Setter private  String seed;	 @Getter @Setter private  int TTL;  	 @Getter @Setter private  int refreshSpan;  	 @Getter @Setter private  int refreshTTL;          }