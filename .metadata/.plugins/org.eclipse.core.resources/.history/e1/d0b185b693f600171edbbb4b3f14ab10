package com.november.common.token;import org.springframework.boot.context.properties.ConfigurationProperties;import org.springframework.boot.context.properties.EnableConfigurationProperties;import org.springframework.context.annotation.PropertySource;import org.springframework.stereotype.Component;import lombok.Data;@Data@Component@EnableConfigurationProperties@ConfigurationProperties(prefix = "elearn.token.jwt")@PropertySource(value = { "classpath:/security-jwt.properties" }, ignoreResourceNotFound = true)public class TokenConfig {		private  String issuer;    private  String seed;    private  int TTL;      private  int refreshSpan;      private  int refreshTTL;          }